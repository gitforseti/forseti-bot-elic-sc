{
    "name": "forseti/forseti-carga-sigaes",
    "description": "carga de licitacoes do portal elic-sc",
    "type": "project",
    "authors": [
        {
            "name": "Antonio Augusto D'Agostino",
            "email": "antonio.augusto@forseti.com.br"
        }
    ],

    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.forseti.com.br"
        }
    ],

    "require": {
        "php" : ">=5.6.0",
        "illuminate/database": "^5.3",
        "illuminate/events": "^5.3",
        "symfony/console": "^3.1",
        "monolog/monolog": "^1.20",
        "php-di/php-di": "^5.3",
        "robmorgan/phinx": "^0.6.2",
        "sentry/sentry": "^1.6",
        "forseti/logger": "^1.2",
        "forseti/bot-elic-sc": "dev-master",
        "forseti/elicitacao-sync-carga" : "1.*"

    },

    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },

    "autoload": {
        "psr-4": {
            "Forseti\\Carga\\ElicSC\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Forseti\\Carga\\ElicSC\\Test\\": "tests/"
        }
    }
}
